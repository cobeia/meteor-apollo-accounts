{
  "name": "meteor-apollo-accounts-ssr",
  "version": "2.1.0",
  "description": "Meteor accounts SSR in GraphQL",
  "main": "./client",
  "repository": {
    "url": "https://github.com/nicolaslopezj/meteor-apollo-accounts-ssr",
    "type": "git"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./client",
    "prepublish": "npm run build",
    "test": "npm run lint && jest",
    "watch": "watch npm\\ run\\ build ./src",
    "test-watch": "jest --watch",
    "lint": "standard \"src/**/*.js\""
  },
  "keywords": [
    "meteor",
    "apollo",
    "graphql",
    "accounts",
    "react-native"
  ],
  "author": "abathellier",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "eslint": ">=3.0.0",
    "graphql-tag": "^2.9.2",
    "jest-cli": "^23.2.0",
    "standard": "*"
  },
  "peerDependencies": {
    "graphql-tag": "^2.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/client/"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.51",
    "graphql": "^0.13.2",
    "uglify-js": "^3.4.3",
    "underscore": "^1.9.1"
  },
  "standard": {
    "globals": [
      "it",
      "xit",
      "expect",
      "test",
      "jest"
    ]
  }
}
